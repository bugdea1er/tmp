#ifndef TMP_EXPORT_H
#define TMP_EXPORT_H

#if defined _WIN32 || defined __CYGWIN__
#ifdef TMP_BUILDING_DLL
#ifdef __GNUC__
#define TMP_EXPORT __attribute__((dllexport))
#else
#define TMP_EXPORT __declspec(dllexport)
#endif
#else
#ifdef __GNUC__
#define TMP_EXPORT __attribute__((dllimport))
#else
#define TMP_EXPORT __declspec(dllimport)
#endif
#endif
#define TMP_NO_EXPORT
#else
#if __GNUC__ >= 4
#define TMP_EXPORT __attribute__((visibility("default")))
#define TMP_NO_EXPORT __attribute__((visibility("hidden")))
#else
#define TMP_EXPORT
#define TMP_NO_EXPORT
#endif
#endif

#endif /* TMP_EXPORT_H */
